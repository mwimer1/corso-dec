    {
      "label": "âš¡ Quick Dev Start",
      "type": "shell",
      "command": "pnpm dev",
      "isBackground": true,
      "group": "build",
      "runOptions": {
        "runOn": "default"
      },
      "problemMatcher": [
        {
          "owner": "typescript",
          "pattern": {
            "regexp": "^([^\\s].*)\\((\\d+|\\d+,\\d+|\\d+,\\d+,\\d+,\\d+)\\):\\s+(error|warning|info)\\s+(TS\\d+)\\s*:\\s*(.*)$",
            "file": 1,
            "location": 2,
            "severity": 3,
            "code": 4,
            "message": 5
          },
          "background": {
            "activeOnStart": true,
            "beginsPattern": "^started server on .+, url",
            "endsPattern": "^waiting on"
          }
        }
      ],
      "presentation": {
        "reveal": "always",
        "panel": "new",
        "scrollToBottom": true
      }
    },
    {
