    "lint:no-clerkprovider-outside-root": "pnpm exec eslint --rule 'corso/no-clerkprovider-outside-root: error' '**/*.{ts,tsx}'",
    "lint:api-edge-barrel-no-server-exports": "pnpm exec eslint --rule 'corso/api-edge-barrel-no-server-exports: error' 'lib/api/**/*.{ts,tsx}'",
    "lint:nextjs15-route-params-optimization": "pnpm exec eslint --rule 'corso/nextjs15-route-params-optimization: error' 'app/api/**/*.{ts,tsx}'",
    "hardening:scan": "rg -n --glob '!node_modules' \"from 'next/headers'|from \\\"next/headers\\\"\" lib || true && rg -n --glob '!node_modules' 'process\\.env\\.' . | rg -v 'lib/env/server\\.ts' || true",
    "hardening:verify": "pnpm typecheck:all && pnpm lint && pnpm build",
    "pretypecheck": "pnpm openapi:gen",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "typecheck:all": "tsc -b config/typescript/tsconfig.json config/typescript/tsconfig.testing.json",
    "typecheck:project-refs": "tsc -b config/typescript/tsconfig.json",
    "typecheck:fallback": "rimraf config/typescript/*.tsbuildinfo tsconfig.tsbuildinfo && tsc --noEmit --project tsconfig.json",
    "typecheck:clean": "rimraf tsconfig.tsbuildinfo && rimraf config/typescript/*.tsbuildinfo && pnpm typecheck",
    "typecheck:verify": "npx tsc --noEmit --project tsconfig.json && echo 'âœ… TypeScript compilation successful'",
