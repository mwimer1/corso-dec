    "deadcode:test-only:ci": "tsx scripts/maintenance/find-test-only-exports.ts --ci",
    "lint:unused": "echo 'âš ï¸  DEPRECATED: Use pnpm validate:dead-code (knip) instead' && pnpm validate:dead-code",
    "lint:edge-runtime": "pnpm run lint:edge",
    "lint:pages-runtime": "tsx scripts/lint/check-pages-runtime.ts",
    "validate": "pnpm run lint:scripts && pnpm run lint:versions && pnpm run lint:lockfile && pnpm run validate:runtime-boundaries && pnpm run verify:routes && pnpm run lint && pnpm run lint:edge && pnpm run typecheck && pnpm run test && pnpm run validate:dead-code:all",
    "validate:lib": "tsx scripts/validation/lib-structure.ts && tsx scripts/maintenance/check-barrels.ts",
    "lint:workflows:pnpm": "node scripts/ci/workflows-consistency-report.mjs && echo ok",
    "codemods:migrate-viewport": "tsx tools/codemods/migrate-viewport.ts",
    "codemods:fix-edge-runtime": "tsx tools/codemods/fix-edge-runtime.ts",
    "lint:metadata-viewport": "tsx scripts/lint/check-metadata-viewport.ts",
    "scripts:sync:utils": "tsx scripts/utils/sync-utils-docs.ts",
    "scripts:verify:utils": "tsx scripts/utils/sync-utils-docs.ts --check",
    "scripts:forbid:scripts-barrels": "tsx scripts/lint/forbid-scripts-barrels.ts",
    "guards:metadata": "tsx scripts/ci/check-metadata.ts",
    "guards:protected": "tsx scripts/ci/check-protected-auth.ts",
    "docs:readmes:check": "tsx scripts/lint/check-readmes.ts",
    "docs:generate:readme": "tsx scripts/maintenance/docs/generate-readmes.ts",
    "docs:links": "tsx scripts/maintenance/validate-docs.ts",
    "openapi:bundle": "redocly bundle api/openapi.yml -o api/openapi.json --ext json",
    "openapi:lint": "spectral lint api/openapi.yml",
    "openapi:types": "openapi-typescript api/openapi.json -o types/api/openapi.d.ts",
    "openapi:gen": "pnpm openapi:bundle && pnpm openapi:lint && pnpm openapi:types",
    "lint:unused:ci": "pnpm ast-grep:scan",
    "validate:unused": "pnpm -w typecheck && pnpm -w lint && pnpm lint:unused:ci",
    "openapi:rbac:check": "tsx scripts/ci/openapi-guard-rbac.ts",
    "openapi:diff": "node scripts/openapi/openapi-diff.mjs",
    "openapi:mock": "pnpm run openapi:gen && pnpm exec prism mock api/openapi.yml",
    "openapi:docs": "pnpm run openapi:gen && pnpm dlx redocly preview-docs api/openapi.yml || echo 'Install redocly if you want local docs'",
    "jscpd:lib+types": "pnpm jscpd:report",
    "jscpd:report": "pnpm dlx jscpd --reporters json --output reports/duplication && echo ok",
    "jscpd:docs": "pnpm dlx jscpd docs --reporters json,console --output reports/jscpd-docs",
    "diag:build": "cross-env NODE_OPTIONS=\"--trace-uncaught --enable-source-maps --unhandled-rejections=strict\" next build",
    "scan:edge-runtime": "pnpm --package=@ast-grep/cli dlx sg scan --rule scripts/rules/ast-grep/runtime-boundaries/ban-server-imports-in-app.yml app",
    "jscpd:sarif": "pnpm dlx jscpd --reporters sarif",
    "jscpd:jsinspect": "pnpm dlx jsinspect app components lib hooks --ignore \"**/*.stories.*\" --ignore \"**/__snapshots__/**\" --threshold 30 --reporter json --no-color > reports/duplication/jsinspect.json",
    "jscpd:jsinspect:sarif": "pnpm run jscpd:jsinspect && pnpm tsx scripts/utils/jsinspect-to-sarif.ts --in reports/duplication/jsinspect.json --out reports/duplication/jsinspect.sarif",
    "lock:refresh": "rimraf pnpm-lock.yaml && pnpm install",
    "jscpd:combined": "pnpm jscpd:report && pnpm jscpd:jsinspect",
    "jscpd:combined:sarif": "pnpm jscpd:sarif && pnpm jscpd:jsinspect:sarif",
    "jscpd:lib+types:plan": "pnpm jscpd:lib+types",
    "ast-grep:scan": "pnpm --package=@ast-grep/cli dlx sg scan --config sgconfig.yml .",
    "validate:ast-grep": "node scripts/lint/ast-grep-validate.mjs",
    "quality:phase1": "pnpm -w typecheck && pnpm -w lint && pnpm -w test && pnpm run guards:guards:rules:phase1",
    "validate:dedupe": "pnpm dlx jscpd --config .jscpd.json",
    "validate:onboarding-boundaries": "pnpm --package=@ast-grep/cli dlx sg scan --rule .astgrep/onboarding/no-widgets-import-outside.yml . && pnpm --package=@ast-grep/cli dlx sg scan --rule .astgrep/onboarding/no-steps-import-flow.yml .",
    "build": "next build && pnpm run typecheck",
    "cleanup:next": "rimraf .next && pnpm next build",
    "predev": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/maintenance/ensure-ports.ps1 -DevPort 3000 -PlaywrightPort 9323",
    "dev": "pnpm run gen:mockdb && pnpm run dev:next",
    "dev:trace-deprecations": "cross-env NODE_OPTIONS=--trace-deprecation pnpm run dev:next",
    "dev:next": "next dev --turbo --hostname localhost -p 3000",
    "dev:clean:orphans": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/maintenance/kill-orphans.ps1 -MaxAgeMinutes 240",
    "cleanup:ports": "powershell -NoProfile -ExecutionPolicy Bypass -File scripts/maintenance/ensure-ports.ps1 3000 9323 -KillOnly",
    "dev:start": "node -e \"require('child_process').spawn('pnpm', ['dev'], {stdio: 'inherit', detached: true})\"",
    "start": "next start",
    "tailwind:build": "tailwindcss -c tailwind.config.ts -i styles/globals.css -o styles/build/tailwind.css --minify",
    "tailwind:watch": "tailwindcss -c tailwind.config.ts -i styles/globals.css -o styles/build/tailwind.css --watch",
    "pretest": "pnpm --filter @corso/eslint-plugin run build",
    "validate:dup": "pnpm dlx jscpd --config .jscpd.json",
    "validate:duplication": "pnpm dlx jscpd --config .jscpd.json",
    "validate:boundaries:client-server": "tsx scripts/maintenance/boundaries/check-cross-boundary.ts",
    "validate:zod:strict": "tsx scripts/maintenance/zod/strict-audit.ts",
    "validate:boundaries:deep": "tsx scripts/maintenance/boundaries/check-deep-imports.ts",
    "codemods:server-to-integrations": "tsx scripts/maintenance/codemods/replace-server-to-integrations.ts",
    "test": "node scripts/assert-no-colocated-tests.cjs && vitest --run",
    "test:security": "vitest --run tests/security",
    "jscpd:tests": "pnpm dlx jscpd tests --reporters console --format typescript --format tsx --min-lines 15",
    "lint:dup-tests-ci": "pnpm jscpd:tests",
    "lint": "pnpm exec eslint . --cache --cache-location node_modules/.cache/eslint/.eslintcache --cache-strategy content && pnpm ast-grep:scan",
    "lint:eslint": "pnpm exec eslint .",
    "lint:ast-grep": "pnpm run ast-grep:scan",
    "lint:ci": "pnpm run -s lint:eslint --max-warnings=0 && pnpm run -s lint:ast-grep",
    "lint:menu-no-any": "pnpm --package=@ast-grep/cli dlx sg scan --rule scripts/rules/ast-grep/menu-no-any.yml .",
    "lint:filenames": "node -e \"require('glob').globSync('**/*.*',{ignore:['**/node_modules/**','**/.next/**','**/dist/**','**/build/**']}).forEach(f=>require('child_process').spawnSync('tsx',['scripts/lint/check-filename-case.ts',f],{stdio:'inherit'}) )\"",
    "lint:circular": "madge --circular --extensions ts ./types ./lib",
    "dep:check": "madge --circular --extensions ts,tsx contexts/",
    "lint:css": "stylelint --config config/.stylelintrc.cjs \"styles/**/*.{css,scss}\" --cache",
    "lint:css:fix": "stylelint --config config/.stylelintrc.cjs \"styles/**/*.{css,scss}\" --fix --cache",
    "lint:css:guard": "node scripts/lint-css-global-guard.cjs",
