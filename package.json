    "lint:full": "pnpm -F @corso/eslint-plugin run build && pnpm run lint:scripts && pnpm lint",
    "lint:versions": "tsx scripts/lint/check-runtime-versions.ts",
    "lint:lockfile": "tsx scripts/lint/check-lockfile-major.ts",
