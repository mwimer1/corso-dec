    "lint:dup:dlx": "pnpm dlx jscpd --pattern \"styles/**/*.{css,ts}\" --reporters console --min-lines 10 --absolute",
    "lint:all": "pnpm lint && pnpm lint:css:dlx && pnpm lint:dup:dlx",
    "guard:styles": "pnpm --package=@ast-grep/cli dlx sg scan --config .config/ast-grep/styles.yml .",
