    "audit:performance": "echo 'audit-bundle-performance.js not found - bundle analysis handled by check-bundle-size.ts'",
    "audit:secrets": "tsx scripts/lint/audit-workflow-secrets.ts",
    "bundlesize": "pnpm exec tsx scripts/ci/check-bundle-size.ts",
    "bundlesize:ci": "pnpm exec tsx scripts/ci/check-bundle-size.ts --json",
    "ci:validate": "pnpm lint:types && pnpm lint:circular && pnpm lint:jsdoc && pnpm lint:edge && pnpm lint:filenames",
    "validate:dependencies": "pnpm exec dependency-cruiser --config .dependency-cruiser.cjs .",
    "validate:commits": "commitlint --from=HEAD~1 --to=HEAD --verbose",
    "validate:fonts": "pnpm exec tsx scripts/lint/no-binary-fonts.ts",
    "validate:package": "pnpm exec tsx scripts/lint/validate-package-json.ts",
    "validate:dead-code": "pnpm exec knip",
