    "validate:styles": "pnpm lint:css && pnpm guard:styles && pnpm guard:bak",
    "lint:css:paths": "pnpm exec tsx scripts/lint/check-css-paths.ts",
    "lint:edge": "pnpm exec tsx scripts/lint/check-edge-compat.ts",
    "lint:fix": "pnpm exec eslint . --fix",
    "guard:no-colocated-tests": "node scripts/assert-no-colocated-tests.cjs",
    "lint:jsdoc": "pnpm exec eslint '**/*.ts' --rule 'jsdoc/require-jsdoc: error'",
    "lint:md": "pnpm exec markdownlint 'docs/**/*.md' 'README.md' --config .markdownlint.jsonc",
    "lint:types": "pnpm exec eslint \"types/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:lib": "pnpm exec eslint \"lib/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:hooks": "pnpm exec eslint \"hooks/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:actions": "pnpm exec eslint \"actions/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:contexts": "pnpm exec eslint \"contexts/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:app": "pnpm exec eslint \"app/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:components": "pnpm exec eslint \"components/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:styles": "pnpm exec stylelint \"**/*.css\" \"!**/build/**\" \"!**/node_modules/**\" \"!styles/tailwind.css\" --config config/.stylelintrc.cjs --cache",
    "gen:variants": "pnpm exec tsx scripts/maintenance/gen-variants-index.ts",
    "lint:tests": "pnpm exec eslint \"tests/**/*.{ts,tsx}\" --max-warnings=0",
    "lint:sql": "pnpm --package=@ast-grep/cli dlx sg scan --rule supabase/ast-greprc.yml supabase/migrations",
