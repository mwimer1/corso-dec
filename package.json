    "typecheck:tests": "node node_modules/typescript/bin/tsc -p config/typescript/tsconfig.testing.json --noEmit",
    "a11y:contrast": "pnpm exec tsx scripts/lint/contrast-check.ts",
    "a11y:css-size": "pnpm exec tsx scripts/lint/css-size-analyzer.ts",
    "audit:ai": "tsx scripts/lint/audit-ai-security.ts",
    "audit:ci": "pnpm audit --audit-level=high",
    "audit:atomic": "pnpm exec tsx scripts/setup/validate-atomic-design.ts",
