    "audit:styles:fix": "pnpm exec tsx scripts/maintenance/styles-comprehensive-audit.ts --fix",
    "audit:breakpoints": "tsx scripts/lint/audit-breakpoints.ts",
    "audit:tokens:list-unused": "tsx scripts/maintenance/audit-unused-tokens.ts",
    "audit:tokens:audit": "pnpm -w audit:tokens:list-unused",
    "validate:env": "pnpm exec tsx scripts/setup/validate-env.ts",
    "validate:ai-agent-env": "pnpm exec tsx scripts/setup/validate-ai-agent-environment.ts",
    "browsers:update": "node -e \"if(!process.env['CI']){require('child_process').spawnSync('pnpm',['dlx','update-browserslist-db@latest','--update-db'],{stdio:'inherit'})}\"",
