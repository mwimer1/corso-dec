    "lint:hooks:deps": "pnpm exec tsx scripts/lint/validate-effect-deps.ts",
    "lint:hooks:size": "pnpm exec tsx scripts/lint/validate-hook-size.ts",
    "lint:dashboard": "pnpm exec eslint components/dashboard --rule 'corso/dashboard-literal-entity-keys: error'",
    "lint:icons": "pnpm exec eslint components/ui/atoms/icon/**/*.{ts,tsx} --rule 'corso/no-client-in-icons: error'",
    "lint:api-routes": "pnpm exec eslint app/api/**/*.{ts,tsx} --rule 'corso/nextjs15-route-params-async: error'",
    "lint:client-directive": "pnpm exec eslint --rule 'corso/require-client-directive-for-client-code: error' '**/*.{ts,tsx}'",
    "lint:runtime-exports": "pnpm exec eslint --rule 'corso/no-mixed-runtime-exports: error' 'lib/**/*.{ts,tsx}'",
    "lint:no-direct-process-env": "pnpm exec eslint --rule 'corso/no-direct-process-env: error' '**/*.{ts,tsx}'",
    "lint:server-env-imports": "pnpm exec eslint --rule 'corso/require-server-env-imports: error' 'lib/**/*.ts' 'app/**/*.{ts,tsx}'",
    "lint:no-server-reexports": "pnpm exec eslint --rule 'corso/no-server-reexports: error' 'components/**/*.{ts,tsx}' 'lib/shared/**/*.{ts,tsx}'",
    "lint:no-deprecated-imports": "pnpm exec eslint --rule 'corso/no-deprecated-lib-imports: error' '**/*.{ts,tsx}'",
    "lint:no-runtime-in-types": "pnpm exec eslint --rule 'corso/no-runtime-in-types: error' 'types/**/*.{ts,tsx}'",
    "lint:no-await-headers": "pnpm exec eslint --rule 'corso/no-await-headers: error' '**/*.{ts,tsx}'",
    "lint:no-clerkclient-invoke": "pnpm exec eslint --rule 'corso/no-clerkclient-invoke: error' '**/*.{ts,tsx}'",
    "lint:contexts-barrel-usage": "pnpm exec eslint --rule 'corso/contexts-barrel-usage: error' '**/*.{ts,tsx}'",
